{
  "name": "someskill",
  "version": "0.0.0",
  "description": "An Alexa skill for the Amazon Echo.",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "debug":
      "node --inspect --debug $(which sls) offline start --dontPrintOutput -t -l .webpack/service/src",
    "arn":
      "aws lambda list-functions | jq '.Functions[] | select(.FunctionName | contains(\"someskill\")).FunctionArn'"
  },
  "dependencies": {
    "@types/i18next": "^8.4.4",
    "@types/i18next-sprintf-postprocessor": "^0.0.29",
    "ask-sdk": "^2.0.7",
    "i18next": "^11.3.6",
    "i18next-sprintf-postprocessor": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "@types/webpack": "^4.4.5",
    "@types/webpack-node-externals": "^1.6.0",
    "serverless-bespoken": "^1.0.0",
    "serverless-offline": "^3.16.0",
    "serverless-webpack": "^5.2.0",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^4.4.2",
    "typescript": "^2.4.2",
    "webpack": "^4.15.1",
    "webpack-node-externals": "^1.6.0"
  },
  "files": ["src"],
  "keywords": ["alexa", "alexa-skill", "echo", "skill"]
}
